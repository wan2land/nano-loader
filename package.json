{
  "name": "nano-loader",
  "version": "1.0.0",
  "description": "Dynamic Script Loader for Browser.",
  "keywords": [
    "dynamic import",
    "import",
    "load script",
    "load",
    "script",
    "browser"
  ],
  "author": "Changwan Jun <wan2land@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wan2land/nano-loader.git"
  },
  "bugs": {
    "url": "https://github.com/wan2land/nano-loader/issues"
  },
  "scripts": {
    "build": "npm run build:esm",
    "build:esm": "esbuild index.js --bundle --outfile=index.esm.js --format=esm --minify"
  },
  "module": "./index.esm.js",
  "main": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./index.esm.js"
      },
      "require": {
        "types": "./index.d.ts",
        "default": "./index.js"
      }
    }
  },
  "devDependencies": {
    "esbuild": "^0.17.5"
  }
}
